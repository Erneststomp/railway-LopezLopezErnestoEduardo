<div style="width: 100%;">
    <h1>Register</h1>
   <form action="/register" method="post"  id="register">
    <label>First Name</label>
    <input type='text' name='names' id='names' required/><br>
    <label>Last Name</label>
    <input type='text' name='lastname' id='lastname' required/><br>
    <label>Email</label>
    <input type='email' name='id' id='id' required/><br>
    <label>Age</label>
    <input type='number' name='age'id='age' required/><br>
    <label>Avatar URL</label>
    <input type='url' name='avatar' id='avatar'  required/><br>
    <label>Phone </label>
    <select id="countrycode" name="CountryCode">
        <option value="+1">United States (+1)</option>
        <option value="+52">Mexico (+52)</option>
        <option value="+52">Argentina (+54)</option>
        <option value="+57">Colombia (+57)</option>
        <option value="+34">Spain (+34)</option>
    <input type='number' name='phone' id='phone'  required/><label > </label> <br>
    <label>Address</label>
    <input type='text' name='adress' id='adress' required/><br>
    <label>Alias</label>
    <input type='text' name='alias' id='alias' required/><br>
    <label>Password</label>
    <input type='password' name='password'id='password' required/><br>
    <label>Confirm Password</label>
    <input type='password' name='passwordconf'id='passwordconf' required/><label > </label><br>
    <br>
    <button id="submitButton" name="submit" disabled>Register</button>
    </form>

    <br>
     
    <button id="login">Log In</button>
    
     
</div>
 
    <script src="/socket.io/socket.io.js"></script>    
    <script  src="../../register.js"> </script>