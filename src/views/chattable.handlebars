<div style="width: 100%;" id="generate">
    <div >
        <h1 id="charge">Generate 5 aleatory products</h1>
        <h2>No se guardan, solo los genero</h2>
    </div>


    <div style="display: inline-flex; width: 95%;">
        <div style="width: 90%;">
        <h1> Characters:</h1>
            <div style="display: block;  border: 1px solid red;  padding: 5px;  margin-top: 5px; width: 100%; height: 550px;overflow-y: scroll; border-style:double;">
                 <span id="log_character"></span>
                {{#each characters}}
                    <div>
                        <img src="{{this.thumbnail}}" style="height: 10px;"></img>
                        <h2>Name: {{this.title}}/ Reward: {{this.price}}0 Schmeckles</h2>
                    </div>
                {{/each}}
            </div>
        </div>
    </div>
    
</div>

    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="/socket.io/socket.io.js"></script>    
    <script src="../../test.js"></script>