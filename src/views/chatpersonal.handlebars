<div style="width: 100%;">
    <div style="display: inline-flex; width: 95%;">
        <div style="width: 90%; margin">
            <h1>Personal Chat (only lecture)</h1>
            <label id="namelabel" value={{userData.id}}>You are connected as: {{userData.names}}</label>
            <div style="display: block;  border: 1px solid red;  padding: 5px;  margin-top: 5px; width: 100%; height: 550px;overflow-y: scroll; border-style:double; margin: 30px;">
                <div style="marginRight:'auto',marginLeft:'auto' ">
                    {{#each chats}}
                        <div>
                            <span>{{this.user}} says</span> : <span>{{this.message}}</span><span> at: {{this.date}}</span>
                        </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
</div>

    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="/socket.io/socket.io.js"></script>    